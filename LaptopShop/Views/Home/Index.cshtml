                    <div class="product-items">
                       
                        <div class="product-item">
                            @foreach (var item in Model)
                            {
                                <div class="product-image">
                                    <a>
                                        <img class='lazy' onerror="this.src = '@item.Image';" data-src='@item.Image' alt='Lenovo Legion 5 17″ Gen 6 (AMD) (2022)' />
                                    </a>

                                </div>
                                <div class="product-desc">
                                    <h3 class="product-name"><a asp-controller="Home" asp-action="Product_details" asp-route-id="@item.Id">Lenovo Legion 5 17″ Gen 6 (AMD) (2022)</a></h3>
